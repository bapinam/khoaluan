@using KhoaLuan.Data.Enums;
@{ ViewData["Title"] = "Chế Biến";
    Layout = "~/Views/Shared/_Layout.cshtml"; }
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6" style="margin-bottom:8px; ">
                <h1>Chế Biến</h1>
            </div>
            <div class="col-sm-6">
                <div class="float-sm-right" style="margin-bottom:5px;">
                    <button type="button" onclick="window.location.href='/ProcessingDetails/Index'" class="btn btn-dark">
                        <i class="fa fa-refresh"></i>
                    </button>
                </div>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!--Phát nguyên vật liệu-->
@await Html.PartialAsync("DistributingMaterials")
<!--Đánh dấu chế biến-->
@await Html.PartialAsync("MarkProcessing")
<!--Hoàn Thành Chế Biến-->
@await Html.PartialAsync("ProcessingComplete")